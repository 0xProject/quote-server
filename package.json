{
    "name": "@0x/quote-server",
    "version": "6.0.6",
    "description": "RFQ Quote server",
    "main": "lib/src/index.js",
    "types": "lib/src/index.d.ts",
    "author": "Steve Klebanoff, Fabio Berger",
    "license": "Apache-2.0",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc --watch",
        "clean": "shx rm -rf lib",
        "tslint": "tslint --format stylish --project .",
        "tslint:fix": "yarn tslint --fix",
        "prettier": "prettier '**/*.{ts,tsx,json,md}' --config .prettierrc",
        "prettier:fix": "yarn prettier --write",
        "prettier:lint": "yarn prettier --list-different",
        "lint": "run-p tslint prettier:lint",
        "fix": "run-s prettier:fix tslint:fix",
        "test": "NODE_ENV=test mocha --require source-map-support/register --require make-promises-safe ./lib/test/**/**/*_test.js --exit",
        "test:all": "yarn test && yarn lint"
    },
    "devDependencies": {
        "@0x/tslint-config": "^4.0.0",
        "@0x/typescript-typings": "^4.1.0",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.0.0",
        "chai": "^4.0.1",
        "chai-as-promised": "^7.1.0",
        "chai-bignumber": "^3.0.0",
        "dirty-chai": "^2.0.1",
        "make-promises-safe": "^1.1.0",
        "mocha": "^6.0.2",
        "node-mocks-http": "^1.8.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.16.3",
        "shx": "^0.3.2",
        "ts-node": "^8.6.2",
        "tslint": "^6.0.0",
        "typemoq": "^2.1.0",
        "typescript": "3.0.1"
    },
    "dependencies": {
        "@0x/json-schemas": "^6.4.0",
        "@0x/order-utils": "^10.2.4",
        "@0x/protocol-utils": "^1.9.0",
        "@0x/utils": "^5.4.1",
        "@types/express": "^4.17.3",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "http-status-codes": "^1.4.0"
    }
}
